<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- **************************** -->
	<!-- ERP to hybris delivery qty number mapping -->
	<!-- **************************** -->

	<alias name="defaultGreenleeDeliveryQtyCompositionHandler" alias="greenleeDeliveryQtyCompositionHandler" />
	<bean id="defaultGreenleeDeliveryQtyCompositionHandler"
		class="com.greenlee.datahub.inbound.delivery.publication.impl.DefaultDlyQtyCompositionHandler"
		parent="sapAbstractOrderCompositionHandler" />

	<alias name="defaultGreenleeGoodsIssueQtyCompositionHandler" alias="greenleeGoodsIssueQtyCompositionHandler" />
	<bean id="defaultGreenleeGoodsIssueQtyCompositionHandler"
		class="com.greenlee.datahub.inbound.delivery.publication.impl.DefaultGreenleeGoodsIssueQtyCompositionHandler"
		parent="sapAbstractOrderCompositionHandler" />
		
</beans>
