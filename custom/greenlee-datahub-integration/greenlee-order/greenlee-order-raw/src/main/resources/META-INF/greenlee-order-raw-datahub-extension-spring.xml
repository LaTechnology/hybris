<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="sapRawHybrisOrderCanonicalOrderSDComponentKeys" class="com.hybris.datahub.sapidocintegration.grouping.FilterKeysGroupingHandler">
		<property name="canonicalItem" value="CanonicalOrderSDComponent" />
		<property name="rawItem" value="RawHybrisOrder" />
		<property name="rawItemAttributes">
			<set>
				<value>orderId</value>
				<value>textLine</value>
				<value>textId</value>
			</set>
		</property>
	</bean>

	<alias name="sapDefaultCompositionHandlerOrderItemCancelInbound" alias="sapCompositionHandlerOrderItemCancelInbound" />
	<bean id="sapDefaultCompositionHandlerOrderItemCancelInbound" class="com.greenlee.datahub.order.composition.impl.DefaultGreenleeCompositionHandlerOrderItemCancelInbound"
		parent="sapAbstractOrderCompositionHandler" />
		
</beans>