	<rawItems>
		<item>
			<type>RawHybrisOrder</type>
			<description>FullOrder</description>
			<attributes>
				<attribute><name>textLine</name></attribute>
				<attribute><name>textId</name></attribute>
				<attribute><name>salesOrg</name></attribute>
				<attribute><name>orderType</name></attribute>
				<attribute><name>poType</name></attribute>
				<attribute><name>incoterms1</name></attribute>
				<attribute><name>incoterms2</name></attribute>
				<attribute><name>shipByDate</name></attribute>
				<attribute><name>orderer</name></attribute>
				<attribute><name>addrNotes</name></attribute>
			</attributes>
		</item>
		<item>
			<type>RawORDERS</type>
			<description>Order Data coming SAP ERP (IDoc Type ORDERS05)</description>
			<attributes>
				<attribute><name>E1EDKA1-PARVW</name></attribute>
				<attribute><name>E1EDKA1-IHREZ</name></attribute><!-- Hybris order id -->
				<attribute><name>E1EDP01-ZGHS_ORD_ITEMS-ZG_CANCEL_QTY</name></attribute>
			</attributes>
		</item>
		<item>
			<type>RawDELVRY</type>
			<description>Delivery and Goods Issue data coming by IDoc Type DELVRY07</description>
			<attributes>
				<attribute><name>E1EDL20-ZGS_DELV_E1EDL20-ZG_TRACKING_INFO</name></attribute><!-- Array of Tracking numbers.  --> 
				<attribute><name>E1EDL20-E1EDL37-MOVE_STATUS</name></attribute> <!-- PGIed if 0050 -->
				<!-- <attribute><name>E1EDL20-E1EDL22-VSBED_BEZ</name></attribute> --> <!-- shipping carrier -->
				<attribute><name>E1EDL20-E1EDL37-E1EDL49-TRACKN</name></attribute> <!-- tracking number -->
				<!-- <attribute><name>E1EDL20-E1EDL24-POSNR</name></attribute> --><!-- delivery item number -->
				<attribute><name>E1EDL20-E1EDL24-LFIMG</name></attribute><!-- qty -->  
				<attribute><name>E1EDL20-E1EDL24-VGPOS</name></attribute><!-- line item number -->  
				<attribute><name>E1EDL20-VBELN</name></attribute><!-- delivery number -->  
				<attribute><name>E1EDL20-ZGS_DELV_E1EDL20-ZG_NETVALUE</name></attribute><!-- Consignment Value -->  
				<attribute><name>E1EDL20-ZGS_DELV_E1EDL20-ZG_TAX</name></attribute><!-- Tax Break-up -->  
				<attribute><name>E1EDL20-ZGS_DELV_E1EDL20-ZG_DUTY</name></attribute><!-- Duty Break-up -->  
				<attribute><name>E1EDL20-ZGS_DELV_E1EDL20-ZG_PROMOTION</name></attribute><!-- Promotion Break-up -->  
				<attribute><name>E1EDL20-ZGS_DELV_E1EDL20-ZG_FREIGHT</name></attribute><!-- Freight Break-up -->  
			</attributes>
		</item>
	</rawItems>

