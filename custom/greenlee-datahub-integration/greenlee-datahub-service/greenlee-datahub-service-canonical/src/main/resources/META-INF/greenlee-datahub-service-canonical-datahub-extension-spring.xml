<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

	<alias name="greenleePublicationErrorDataToModelConverter" alias="publicationErrorDataToModelConverter"/>
	<bean name="greenleePublicationErrorDataToModelConverter"
		  class="com.greenlee.datahub.service.conversion.publication.GreenleePublicationErrorDataToModelConverter">
		<property name="dataHubPublicationErrorRepository" ref="dataHubPublicationErrorRepository" />
		<property name="canonicalItemRepository" ref="canonicalItemRepository" />
	</bean>
	
	<alias name="greenleeCompositionErrorModelToDataPopulator" alias="compositionErrorModelToDataPopulator"/>
	<bean name="greenleeCompositionErrorModelToDataPopulator"
		  class="com.greenlee.datahub.service.conversion.composition.GreenleeCompositionErrorModelToDataPopulator" />
		  
</beans>
