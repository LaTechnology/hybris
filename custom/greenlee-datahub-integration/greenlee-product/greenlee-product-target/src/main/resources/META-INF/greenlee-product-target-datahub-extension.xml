<extension xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.hybris.com/schema/"
	xsi:schemaLocation="http://www.hybris.com/schema/ http://www.hybris.com/schema/datahub-metadata-schema-1.2.0.xsd"
	name="sapproduct-target">

	<dependencies>
		<dependency>
			<extension>greenlee-product-canonical</extension>
		</dependency>
		<dependency>
			<extension>sapidocintegration</extension>
		</dependency>
	</dependencies>

	<targetSystems>
		<targetSystem>
			<name>HybrisCore</name>
			<type>HybrisCore</type>
			<exportURL>${targetsystem.hybriscore.url}</exportURL>
			<userName>${targetsystem.hybriscore.username}</userName>
			<password>${targetsystem.hybriscore.password}</password>
			<exportCodes>
				<exportCode>#% impex.setLocale( Locale.ENGLISH )</exportCode>
			</exportCodes>
			<targetItems>
				<item>
					<type>Keyword</type>
					<description>Keyword</description>
					<exportCode>Keyword</exportCode>
					<canonicalItemSource>CanonicalProductKeyword</canonicalItemSource>
					<updatable>true</updatable>
					<status></status>
					<attributes>
						<attribute>
							<name>keyword</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>keyword</transformationExpression>
							<exportCode>keyword[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>catalogVersion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'greenleeProductCatalog:Staged'</transformationExpression>
							<exportCode>catalogVersion(Catalog(id),version)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>language</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>keywordLang.toLowerCase()</transformationExpression>
							<exportCode>language(isoCode)[default=en]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
					</attributes>
				</item>
				<item>
					<type>ClassificationAttributeValue</type>
					<description>ClassificationAttributeValue</description>
					<exportCode>classificationAttributeValue</exportCode>
					<updatable>true</updatable>
					<canonicalItemSource>CanonicalAttributeValue</canonicalItemSource>
					<status></status>
					<attributes>
						<attribute>
							<name>code</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>attributeID + '_' + attributeValueID</transformationExpression>
							<exportCode>code[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>systemVersion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'ERP_CLASSIFICATION_' + categoryType + ':ERP_IMPORT'</transformationExpression>
							<exportCode>systemVersion(Catalog(id),version)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>name</name>
							<localizable>true</localizable>
							<collection>false</collection>
							<transformationExpression>attributeValueName</transformationExpression>
							<exportCode>name</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
					</attributes>
				</item>
				<item>
					<type>ClassificationAttribute</type>
					<description>ClassificationAttribute</description>
					<exportCode>classificationAttribute</exportCode>
					<updatable>true</updatable>
					<canonicalItemSource>CanonicalAttributeCategoryRelation</canonicalItemSource>
					<status></status>
					<dependencies>
						<dependency>ClassificationAttributeValue</dependency>
					</dependencies>
					<attributes>
						<attribute>
							<name>code</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>attributeID</transformationExpression>
							<exportCode>code[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>systemVersion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'ERP_CLASSIFICATION_' + categoryType + ':ERP_IMPORT'</transformationExpression>
							<exportCode>systemVersion(catalog(id),version)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>name</name>
							<localizable>true</localizable>
							<collection>false</collection>
							<transformationExpression>resolve('CanonicalAttribute').attributeName</transformationExpression>
							<exportCode>name</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapERPCharacteristicLongText</name>
							<localizable>true</localizable>
							<collection>false</collection>
							<transformationExpression>resolve('CanonicalAttribute').attributeDescription</transformationExpression>
							<exportCode>sapERPCharacteristicLongText</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>defaultAttributeValues</name>
							<localizable>false</localizable>
							<collection>true</collection>
							<transformationExpression>resolve('CanonicalAttribute').attributeValueID?.!['ERP_CLASSIFICATION_' + #root.categoryType + ':ERP_IMPORT:' + #root.attributeID + '_' + #this]</transformationExpression>
							<exportCode>defaultAttributeValues(systemVersion(catalog(id),version),code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
					</attributes>
				</item>
				<item>
					<exportCode>ClassificationClass</exportCode>
					<type>ClassificationClass</type>
					<description>Classifying Category</description>
					<updatable>true</updatable>
					<canonicalItemSource>CanonicalCategory</canonicalItemSource>
					<status></status>
					<attributes>
						<attribute>
							<name>code</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>categoryID</transformationExpression>
							<exportCode>code[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>catalogVersion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'ERP_CLASSIFICATION_' + categoryType + ':ERP_IMPORT'</transformationExpression>
							<exportCode>catalogVersion(Catalog(id),version)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>name</name>
							<localizable>true</localizable>
							<collection>false</collection>
							<transformationExpression>categoryName</transformationExpression>
							<exportCode>name</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>supercategories</name>
							<localizable>false</localizable>
							<collection>true</collection>
							<transformationExpression>categoryParent == null ? null : {categoryParent+':ERP_CLASSIFICATION_'+categoryType+':ERP_IMPORT'}</transformationExpression>
							<exportCode>supercategories(code,catalogVersion(catalog(id),version))</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
					</attributes>
				</item>
				<item>
					<exportCode>ClassAttributeAssignment</exportCode>
					<type>ClassAttributeAssignment</type>
					<description>Create Category Feature</description>
					<updatable>true</updatable>
					<canonicalItemSource>CanonicalAttributeCategoryRelation</canonicalItemSource>
					<status></status>
					<dependencies>
						<dependency>ClassificationAttribute</dependency>
						<dependency>ClassificationClass</dependency>
					</dependencies>
					<attributes>
						<attribute>
							<name>classificationClass</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'ERP_CLASSIFICATION_' + categoryType + ':ERP_IMPORT:' + categoryID</transformationExpression>
							<exportCode>classificationClass(catalogVersion(catalog(id),version),code)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>classificationAttribute</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'ERP_CLASSIFICATION_' + categoryType + ':ERP_IMPORT:' + attributeID</transformationExpression>
							<exportCode>classificationAttribute(systemVersion(catalog(id),version),code)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>attributeType</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>resolve('CanonicalAttribute').attributeValueID?.size() > 0 ? 'enum' : resolve('CanonicalAttribute').attributeType == 'DATE' ? 'date' : resolve('CanonicalAttribute').attributeType == 'NUM' ? 'number' : 'string'</transformationExpression>
							<exportCode>attributeType(code)</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>multiValued</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>!(resolve('CanonicalAttribute').singleValued?.trim()?.length() >= 0)</transformationExpression>
							<exportCode>multiValued</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>unit</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>resolve('CanonicalAttribute').attributeType != 'CURR' and resolve('CanonicalAttribute').unit != null ? resolve('CanonicalAttribute').unit + ':ERP_CLASSIFICATION_' + categoryType + ':ERP_IMPORT:' + resolve('CanonicalAttribute').resolve('CanonicalHybrisUnit','${sapcoreconfiguration.pool}').unitType : ''</transformationExpression>
							<exportCode>unit(code,systemVersion(catalog(id),version),unitType)[unique=true]</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
					</attributes>
				</item>
				<item>
					<exportCode>GreenleeProduct</exportCode>
					<type>BaseProduct</type>
					<description>Products</description>
					<updatable>true</updatable>
					<canonicalItemSource>CanonicalBaseProduct</canonicalItemSource>
					<status></status>
					<dependencies>
						<dependency>ClassAttributeAssignment</dependency>
					</dependencies>
					<attributes>
						<attribute>
							<name>code</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>productID</transformationExpression>
							<exportCode>code[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>catalogVersion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'greenleeProductCatalog:Staged'</transformationExpression>
							<exportCode>catalogVersion(Catalog(id),version)[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>name</name>
							<localizable>true</localizable>
							<collection>false</collection>
							<transformationExpression>productName</transformationExpression>
							<exportCode>name</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>unit</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>unit</transformationExpression>
							<exportCode>unit(code)</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapConfigurable</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'X'.equalsIgnoreCase(configurable)</transformationExpression>
							<exportCode>sapConfigurable</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>variantType</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'X'.equalsIgnoreCase(configurable) ? 'ERPVariantProduct' : ''</transformationExpression>
							<exportCode>variantType(code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapBlocked</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>blocked?.trim()?.length() > 0</transformationExpression>
							<exportCode>sapBlocked</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapBlockedDate</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>blockedFrom.replace('00000000','')</transformationExpression>
							<exportCode>sapBlockedDate[dateformat='yyyyMMdd']</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapEAN</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>EAN?:resolve('CanonicalProductUnit')?.EAN?:''</transformationExpression>
							<exportCode>sapEAN</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapBaseUnitConversion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'1'</transformationExpression>
							<exportCode>sapBaseUnitConversion</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>supercategories</name>
							<localizable>false</localizable>
							<collection>true</collection>
							<transformationExpression>determineSupercategories(productID)</transformationExpression>
							<exportCode>supercategories(code,catalogVersion(catalog(id),version))</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
					</attributes>
				</item>
				<item>
					<exportCode>GreenleeProduct</exportCode>
					<type>SalesProduct</type>
					<description>Products</description>
					<updatable>true</updatable>
					<canonicalItemSource>CanonicalSalesProduct</canonicalItemSource>
					<status></status>
					<dependencies>
						<dependency>ClassAttributeAssignment</dependency>
					</dependencies>
					<attributes>
						<attribute>
							<name>code</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>productID</transformationExpression>
							<exportCode>code[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>catalogVersion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>resolve('CanonicalProductSalesAreaToCatalogMapping','${sapcoreconfiguration.pool}').catalogVersion</transformationExpression>
							<exportCode>catalogVersion[unique=true]</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>name</name>
							<localizable>true</localizable>
							<collection>false</collection>
							<transformationExpression>resolve('CanonicalBaseProduct').productName</transformationExpression>
							<exportCode>name</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>unit</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>unit?:resolve('CanonicalBaseProduct').unit</transformationExpression>
							<exportCode>unit(code)</exportCode>
							<mandatoryInHeader>true</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapConfigurable</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'X'.equalsIgnoreCase(resolve('CanonicalBaseProduct').configurable)</transformationExpression>
							<exportCode>sapConfigurable</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>variantType</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>'X'.equalsIgnoreCase(resolve('CanonicalBaseProduct').configurable) ? 'ERPVariantProduct' : ''</transformationExpression>
							<exportCode>variantType(code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapBlocked</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>blocked?.trim()?.length() > 0 or resolve('CanonicalBaseProduct').blocked?.trim()?.length() > 0</transformationExpression>
							<exportCode>sapBlocked</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapBlockedDate</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>new java.util.PriorityQueue({resolve('CanonicalBaseProduct').blockedFrom,blockedFrom}.?[#this!='00000000']).peek()?:''</transformationExpression>
							<exportCode>sapBlockedDate[dateformat='yyyyMMdd']</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapEAN</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>unit !=null and !unit.isEmpty() ? resolve('CanonicalProductUnit').EAN?:'' : resolve('CanonicalBaseProduct').EAN?: resolve('CanonicalBaseProduct').resolve('CanonicalProductUnit')?.EAN?:''</transformationExpression>
							<exportCode>sapEAN</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapBaseUnitConversion</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>unit == null or unit.isEmpty() ? 1 : T(java.lang.Double).parseDouble(resolve('CanonicalProductUnit').conversionToBaseUnitNumerator) / T(java.lang.Double).parseDouble(resolve('CanonicalProductUnit').conversionToBaseUnitDenominator)</transformationExpression>
							<exportCode>sapBaseUnitConversion</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute disabled="true">
							<name>Europe1PriceFactory_PPG</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>pricingGroup?:''</transformationExpression>
							<exportCode>Europe1PriceFactory_PPG(code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute disabled="true">
							<name>Europe1PriceFactory_PDG</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>pricingGroup?:''</transformationExpression>
							<exportCode>Europe1PriceFactory_PDG(code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>supercategories</name>
							<localizable>false</localizable>
							<collection>true</collection>
							<transformationExpression>determineSupercategories(productID)</transformationExpression>
							<exportCode>supercategories(code,catalogVersion(catalog(id),version))</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute disabled="true">
							<name>Europe1PriceFactory_PTG</name>
