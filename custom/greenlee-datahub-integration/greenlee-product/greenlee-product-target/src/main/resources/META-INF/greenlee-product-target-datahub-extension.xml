							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>minOrderQuantity</transformationExpression>
							<exportCode>minOrderQuantity</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapFamilyID</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>sapFamilyID</transformationExpression>
							<exportCode>sapFamilyID</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>commissionGroupCode</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>commissionGroupCode</transformationExpression>
							<exportCode>commissionGroupCode</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						
