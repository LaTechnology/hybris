							<collection>false</collection>
							<transformationExpression>determineTax(resolve('CanonicalProductSalesAreaToCatalogMapping','${sapcoreconfiguration.pool}').country)</transformationExpression>
							<exportCode>Europe1PriceFactory_PTG(code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
							<name>sapPlant</name>
							<localizable>false</localizable>
							<collection>false</collection>
							<transformationExpression>plant</transformationExpression>
							<exportCode>sapPlant(code)</exportCode>
							<mandatoryInHeader>false</mandatoryInHeader>
						</attribute>
						<attribute>
